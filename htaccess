RewriteEngine On
RewriteCond %{SERVER_PORT} !^443$
RewriteRule .* https://%{SERVER_NAME}%{REQUEST_URI} [R,L]

//Rewrite to www
Options +FollowSymLinks
RewriteEngine on
RewriteCond %{HTTP_HOST} ^https://azs-rosservis.ru/[nc]
RewriteRule ^(.*)$ http://www.https://azs-rosservis.ru//$1 [r=301,nc]

//Страница ошибки 404
ErrorDocument 404 https://azs-rosservis.ru/error
